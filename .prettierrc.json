{
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-router/(.*)$)|^(react-router$)",
    "^(next/(.*)$)|^(next$)",
    "^(zustand/(.*)$)|^(zustand$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@workspace/(.*)$",
    "",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/components/dialogs/(.*)$",
    "^@/components/options/(.*)$",
    "^@/components/sections/(.*)$",
    "",
    "^@/pages/(.*)$",
    "",
    "^@/config/(.*)$",
    "^@/store/(.*)$",
    "^@/providers/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils/(.*)$",
    "^@/hooks/(.*)$",
    "",
    "^lucide-react",
    "",
    "^types$",
    "^@/types/(.*)$",
    "",
    "^./components/(.*)$",
    "",
    "(.*)\\.css$",
    "^@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderCaseSensitive": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],

  "tailwindStylesheet": "./src/index.css",
  "tailwindFunctions": ["clsx", "cva", "cn"],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
